    "lodash": "^4.17.11",
    "masonry-layout": "^4.1.1",
    "moment": "^2.22.0",
    "ng-annotate-loader": "^0.6.1",
    "ngtemplate-loader": "^2.0.1",
    "node-libs-browser": "2.0.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.3.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^2.1.3",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.3",
    "sass-loader": "6.0.6",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.4.0",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^4.1.1",
    "typescript": "^3.1.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^3.1.11",
    "webpack-externals-plugin": "^1.0.0",
    "webpack-merge": "^0.15.0",
    "yargs": "^12.0.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
