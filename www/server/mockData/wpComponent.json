      "pageSubTitle": "",
      "pageHeroImage": "spark-ui-bkgd-1.png",
      "mainDescription": "<p class=\"p1\"><span class=\"s1\">Alerts can notify the user about information, an event, an action, an error or a warning.</span></p>\n",
      "mainImage": false,
      "designSections": [
          {
              "sectionContent": [
                  {
                      "acf_fc_layout": "flexible_section_title",
                      "sectionTitleText": "Style & General Visual Design"
                  },
                  {
                      "acf_fc_layout": "flexible_section_body",
                      "sectionBodyContent": "<p><span class=\"s1\"><b>CSS Style for Alerts</b></span></p>\n<ul>\n<li class=\"p2\"><span class=\"s1\">font-family: CiscoSans-Regular;</span></li>\n<li class=\"p2\"><span class=\"s1\">font-size (title): 16px;</span></li>\n<li class=\"p2\"><span class=\"s1\">font-size (body): 14px;</span></li>\n<li class=\"p2\"><span class=\"s1\">text (body): #666666;</span></li>\n<li class=\"p2\"><span class=\"s1\">background-color: #FFFFFF;</span></li>\n<li class=\"p2\"><span class=\"s1\">border: #E0E0E0, 1px, outside</span></li>\n<li class=\"p2\"><span class=\"s1\">letter-spacing: 0px;</span></li>\n<li class=\"p2\"><span class=\"s1\">corner-radius: 4px</span></li>\n</ul>\n"
                  },
